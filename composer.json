{
    "name": "mlusas/laravel-loom-updates",
    "description": "A Laravel package for tracking Loom video updates in your codebase",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Mitch Lusas",
            "email": "mlusas@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "mlusas\\LaravelLoomUpdates\\": "src/",
            "mlusas\\LaravelLoomUpdates\\Database\\Migrations\\": "database/migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mlusas\\LaravelLoomUpdates\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mlusas\\LaravelLoomUpdates\\LoomUpdatesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
